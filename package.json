{
  "name": "lightelligence-javascript-sdk",
  "version": "0.1.0",
  "description": "javascript SDK for lightelligence platform",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack --config webpack.config.js --watch true",
    "test": "jest --coverage",
    "lint": "eslint src/** test/**",
    "documentation": "documentation build src/** -f html -o docs --theme lightelligence_theme",
    "build": "rm -rf dist && webpack --config webpack.config.js",
    "license-report": "legally",
    "precommit": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/lightelligence/javascript-sdk.git"
  },
  "keywords": [
    "osram",
    "lightelligence",
    "iot",
    "js",
    "sdk"
  ],
  "author": "Roman Makovyak",
  "license": "MIT",
  "homepage": "https://bitbucket.org/lightelligence/javascript-sdk#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.5",
    "documentation": "^8.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "husky": "^0.14.3",
    "install": "^0.12.1",
    "jest": "^24.1.0",
    "jest-fetch-mock": "^1.6.5",
    "jest-localstorage-mock": "^2.2.0",
    "jest-react-native": "^18.0.0",
    "legally": "^2.10.0",
    "metro-react-native-babel-preset": "^0.51.1",
    "npm": "^6.2.0",
    "react-native": "^0.58.3",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "oidc-client": "^1.5.2",
    "postinstall-build": "^5.0.1",
    "react-native-app-auth": "^4.0.0",
    "url-search-params-polyfill": "^5.0.0"
  },
  "jest": {
    "setupFiles": [
      "jest-localstorage-mock",
      "./setupJest.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native-app-auth|react-native|react-native-implementation)/)"
    ],
    "preset": "react-native"
  }
}
